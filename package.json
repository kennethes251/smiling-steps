{
  "name": "smiling-steps-monorepo",
  "version": "1.0.0",
  "description": "Smiling Steps - Mental Health Platform",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "echo 'Build complete'",
    "test": "jest",
    "test:quick": "jest --testPathPattern='core|auth|booking|payment' --maxWorkers=4 --verbose=false",
    "test:stable": "jest --testPathPattern='stable' --verbose",
    "test:integration": "jest --testPathPattern='integration' --runInBand",
    "test:property": "jest --testPathPattern='property' --runInBand",
    "test:full": "jest --testPathPattern='core|integration|stable' --maxWorkers=2",
    "test:deploy": "jest --testPathPattern='stable|core|integration' --runInBand --verbose",
    "test:analyze": "node analyze-tests.js",
    "test:cleanup": "node cleanup-tests.js",
    "lint": "echo 'Linting not configured yet'"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^7.8.7",
    "multer": "^1.4.5-lts.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.15.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "rate-limiter-flexible": "^2.4.2",
    "sequelize": "^6.37.7",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "validator": "^13.15.15",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "fast-check": "^4.5.2",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "mental-health",
    "therapy",
    "counseling"
  ],
  "author": "Smiling Steps Team",
  "license": "MIT"
}
